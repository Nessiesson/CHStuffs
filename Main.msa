*:'/ra' [$] = run('/recompile');
*:'/run' $ = eval($);
*:'/reload' [$] = die(colorize('&cDon\'t do that. :)'));

*:'/exit' [$] = run('/stop');
*:'/quit' [$] = run('/stop');

*:'/tps' [$] = >>>
	@tps = split(', ', split(': ', replace(strip_colors(capture_runas('~console', '/spigot:tps')), '*', ''))[1]);
	msg(@tps);
<<<
